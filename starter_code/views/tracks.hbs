<div class="header">
    <div><h1>TRACKS</h1></div>
    <div><h1>LISTEN</h1></div>
</div>
<div class='tracks-container'>
{{#each tracks.body.items}}
<div>
    <div>
        <h3>{{this.name}}</h3>
    </div>
    <div>
        <audio controls>
        <source src="{{this.preview_url}}" type="audio/ogg">
        </audio>
    </div>
</div>
{{/each}}
</div>